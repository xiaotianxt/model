    "import json\n",
    "\n",
    "with open('./vector.geojson.json', 'rb') as f:\n",
    "    f = json.load(f)\n",
    "\n",
    "f"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "for index, feature in enumerate(f['features']):\n",
    "    x, y, z = feature['geometry']['coordinates']\n",
    "    feature['geometry']['coordinates'] = [x, y]\n",
    "    feature['properties']['z'] = z\n",
    "\n"
   ]
